int motorspeed = 100;

//Создание класса для джойстика
PS2X ps2x;
int error = 0;
byte type = 0;
byte vibrate = 0;

void setup() {

  //Настройка выводов под драйвер
  pinMode(MOTOR_ENA, OUTPUT);
  pinMode(MOTOR_ENB, OUTPUT);
  pinMode(MOTOR_IN1A, OUTPUT);
  pinMode(MOTOR_IN1B, OUTPUT);
  pinMode(MOTOR_IN2A, OUTPUT);
  pinMode(MOTOR_IN2B, OUTPUT);

  //Serial.begin(57600);

  // by default, we'll generate the high voltage from the 3.3v line internally! (neat!)
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);  // initialize with the I2C addr 0x3D (for the 128x64)
  // init done

  // Show image buffer on the display hardware.
  // Since the buffer is intialized with an Adafruit splashscreen
  // internally, this will display the splashscreen.
  display.display();
  delay(2000);

  // Clear the buffer.
  display.clearDisplay();


  //установка выводов и настроек: GamePad(clock, command, attention, data, Pressures?, Rumble?) проверка ошибок
  error = ps2x.config_gamepad(PS2_CLK, PS2_CMD, PS2_SEL, PS2_DAT, pressures, rumble);

}


void loop() {

  //Опрос джойстика
  ps2x.read_gamepad(false, vibrate); //считывание данных с джойстика и установка скорости вибрации


  // Start нажат
  if (ps2x.Button(PSB_START)) {
    //PrintText("Start",2);

  }

  // Select нажат
  if (ps2x.Button(PSB_SELECT)) {
    //PrintText("Select",2);

  }

  // ВВЕРХ нажато
  if (ps2x.Button(PSB_PAD_UP)) {
    //PrintText("UP",2);
    display.clearDisplay();
    display.drawBitmap(0, 0,  angry_eyes, imageWidth, imageHeight, 1);
    display.display();
    StartMotor1_A(motorspeed);
    StartMotor2_A(motorspeed);
  }

  //ВНИЗ нажато
  if (ps2x.Button(PSB_PAD_DOWN)) {
    //PrintText("DOWN",2);
    display.clearDisplay();
    display.drawBitmap(0, 0,  eyes, imageWidth, imageHeight, 1);
    display.display();
    StartMotor1_B(motorspeed);
    StartMotor2_B(motorspeed);

  }

  // Крестовина отпущена
  if ((ps2x.Button(PSB_PAD_UP) == false) & (ps2x.Button(PSB_PAD_DOWN) == false) &
      (ps2x.Button(PSB_PAD_LEFT) == false) & (ps2x.Button(PSB_PAD_RIGHT) == false)) {
    //PrintText("UP",2);
    display.clearDisplay();
    display.drawBitmap(0, 0,  cute_eyes, imageWidth, imageHeight, 1);
    display.display();
    StopMotor1();
    StopMotor2();
  }


  // ВПРАВО нажато
  if (ps2x.Button(PSB_PAD_RIGHT)) {
    //PrintText("RIGHT",2);

    display.clearDisplay();
    display.drawBitmap(0, 0,  wtf_eyes, imageWidth, imageHeight, 1);
    display.display();
    StartMotor1_B(motorspeed);
    StartMotor2_A(motorspeed);


  }

  //ВЛЕВО нажато
  if (ps2x.Button(PSB_PAD_LEFT)) {
    //PrintText("LEFT",2);
    display.clearDisplay();
    display.drawBitmap(0, 0,  wtf_eyes, imageWidth, imageHeight, 1);
    display.display();
    StartMotor1_A(motorspeed);
    StartMotor2_B(motorspeed);
  }


  //vibrate = ps2x.Analog(PSAB_CROSS);  //Скорость вибрации устанавливаеться в зависимости от силы нажатия кнопки (X)

  // L3 нажата
  if (ps2x.ButtonPressed(PSB_L1)) {
    //PrintText("L1",2);
  }

  // R3 нажата
  if (ps2x.Button(PSB_R1)) {
    //PrintText("R1",2);
    if (motorspeed < (255 - Dspeed_const))
    {
      motorspeed = motorspeed + Dspeed_const;
    }
    else motorspeed = motorspeed;

  }

  // L2 нажата
  if (ps2x.ButtonPressed(PSB_L2)) {
    //PrintText("L2",2);
  }

  // R2 нажата
  if (ps2x.Button(PSB_R2)) {
    //PrintText("R2",2);
    if (motorspeed > SPEED_MIN)
    {
      motorspeed = motorspeed - Dspeed_const;
    }
    else motorspeed = motorspeed;
  }

  // Треугольник нажат
  if (ps2x.Button(PSB_TRIANGLE)) {
    //PrintText("TRIANGLE",2);


  }

  //Х нажат
  if (ps2x.Button(PSB_CROSS)) {
    //PrintText("CROSS",2);

  }
  /*
    //Круг нажат
    if (ps2x.ButtonPressed(PSB_CIRCLE)) {
      //PrintText("CIRCLE",2);
    }

    //Квадрат нажат
    if (ps2x.ButtonPressed(PSB_SQUARE)) {
      //PrintText("SQUARE",2);
    }
  */
  /* if (ps2x.Analog(PSS_LY) > 127) {
     StartMotor1_A();
     analogWrite(MOTOR_ENA, (ps2x.Analog(PSS_LY)-128)*2);
    }
     if (ps2x.Analog(PSS_LY) < 128) {
     StartMotor1_B();
     analogWrite(MOTOR_ENA, (127-ps2x.Analog(PSS_LY))*2);
    }*/
}

//Вывод слова на дисплей
void PrintText(char STR[255], char textsize)
{
  display.clearDisplay();
  display.setTextSize(textsize);
  display.setTextColor(WHITE);
  display.setCursor(0, 16);
  display.println(STR);
  display.display();

}

//Запуск двигателей
//Первый, прямой
void StartMotor1_A(char mspeed) {
  //digitalWrite(MOTOR_IN1A, HIGH);
  analogWrite(MOTOR_IN1A, 255);
  digitalWrite(MOTOR_IN1B, LOW);
  analogWrite(MOTOR_ENA, mspeed);
}
//Первый, обратный
void StartMotor1_B(char mspeed) {
  digitalWrite(MOTOR_IN1B, HIGH);
  //digitalWrite(MOTOR_IN1A, LOW);
  analogWrite(MOTOR_IN1A, 0);
  analogWrite(MOTOR_ENA, mspeed);
}
//Второй, прямой
void StartMotor2_A(char mspeed) {
  digitalWrite(MOTOR_IN2A, HIGH);
  //digitalWrite(MOTOR_IN2B, LOW);
  analogWrite(MOTOR_IN2B, 0);
  analogWrite(MOTOR_ENB, mspeed);
}
//Второй, обратный
void StartMotor2_B(char mspeed) {
  //digitalWrite(MOTOR_IN2B, HIGH);
  analogWrite(MOTOR_IN2B, 255);
  digitalWrite(MOTOR_IN2A, LOW);
  analogWrite(MOTOR_ENB, mspeed);
}
//Остановка двигателей
//Первый
void StopMotor1() {
  //digitalWrite(MOTOR_IN1A, LOW);
  analogWrite(MOTOR_IN1A, 0);
  digitalWrite(MOTOR_IN1B, LOW);
}
//Второй
void StopMotor2() {
  digitalWrite(MOTOR_IN2A, LOW);
  //digitalWrite(MOTOR_IN1B, LOW);
  analogWrite(MOTOR_IN2B, 0);
}
